<h1>Gardens#index</h1>
<p>Find me in app/views/gardens/index.html.erb</p>


<div class="row">
    <% @gardens.each do |garden| %>
      <div class="col 3 p-3">
          <%= garden.name %>
          <%= garden.light %>
          <%= garden.size %>
          <%= garden.care_willing %>
          <%= garden.location %>
          <%= garden.latitude %>
          <%= garden.longitude %>
          <%= garden.color %>
        </div>
    <% end %>
</div>
